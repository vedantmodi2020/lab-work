Nth smallest number
*/
#include <algorithm>
#include <iostream>
using namespace std;
int nthsmallest(int data[], int k, int n)
{
    sort(data, data + k);
    return data[n - 1];
}
int main()
{
    cin >> sizeofarray;
    for (int i = 0; i < sizeofarray; ++i)
