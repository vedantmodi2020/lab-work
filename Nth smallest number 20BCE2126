Nth smallest number
*/
#include <algorithm>
#include <iostream>
using namespace std;
int nthsmallest(int data[], int k, int n)
{
    sort(data, data + k);
    return data[n - 1];
}
int main()
{
    int sizeofarray,data[100],n;
    cin >> sizeofarray;
    for (int i = 0; i < sizeofarray; ++i)
    {
        cin >> data[i];
    }
    cout<<"nth number";
    cin>>n;
    int k = sizeofarray;
    cout << "N'th smallest element is " << nthsmallest(data, k, n);
    return 0;
}
